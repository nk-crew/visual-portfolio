{
  "name": "visual-portfolio",
  "title": "Visual Portfolio",
  "version": "3.1.3",
  "description": "Portfolio post type with visual editor",
  "license": "GPL-2.0",
  "author": "nK <https://nkdev.info>",
  "scripts": {
    "dev": "wpeg --build --watch",
    "build": "wpeg --build",
    "production": "wpeg --build --zip",
    "wp-env": "wp-env",
    "env:start": "wp-env start ",
    "env:stop": "wp-env stop",
    "test": "npm-run-all lint test:unit",
    "test:e2e": "playwright test --config tests/e2e/playwright.config.js",
    "test:e2e:ui": "playwright test --config tests/e2e/playwright.config.js --ui",
    "pretest:unit:php": "wp-env start",
    "test:unit:php": "wp-env run --env-cwd='wp-content/plugins/visual-portfolio' tests-wordpress vendor/bin/phpunit -c phpunit.xml.dist --verbose",
    "lint": "concurrently \"npm run lint:js\" \"npm run lint:css\"",
    "lint:css": "stylelint --custom-syntax postcss-scss \"src/**/*.scss\"",
    "lint:js": "eslint src/",
    "prelint:php": "wp-env run --env-cwd='wp-content/plugins/visual-portfolio/src' cli composer update --no-interaction",
    "lint:php": "wp-env run --env-cwd='wp-content/plugins/visual-portfolio/src' cli composer run-script lint",
    "format:css": "stylelint --fix --custom-syntax postcss-scss \"src/**/*.scss\"",
    "format:js": "eslint --fix src/",
    "format:php": "wp-env run --env-cwd='wp-content/plugins/visual-portfolio/src' cli composer run-script format",
    "prepare": "husky install",
    "postinstall": "composer install"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.1",
    "@wordpress/e2e-test-utils-playwright": "^0.3.0",
    "@wordpress/env": "^8.2.0",
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.16.9",
    "@svgr/webpack": "^5.5.0",
    "concurrently": "^8.2.0",
    "eslint-plugin-playwright": "^0.15.2",
    "filenamify": "^4.2.0",
    "base64-inline-loader": "^2.0.1",
    "browserslist-config-nk": "^1.0.0",
    "eslint-config-nk": "^1.1.0",
    "husky": "^7.0.4",
    "json-file": "^0.1.0",
    "lint-staged": "^12.3.2",
    "md5": "^2.3.0",
    "micromatch": "^4.0.4",
    "path": "^0.12.7",
    "po2json": "^0.4.5",
    "prettier-config-nk": "^1.1.0",
    "stylelint-config-nk": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "wpeg": "^1.2.6"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "^7.0.1",
    "@dnd-kit/utilities": "^3.2.0",
    "@fancyapps/fancybox": "^3.5.7",
    "@wordpress/base-styles": "^2.0.1",
    "ace-builds": "^1.4.13",
    "classnames": "^2.3.1",
    "flickr-justified-gallery": "^2.1.2",
    "gutenberg-react-select-styles": "0.0.2",
    "iframe-resizer": "^4.3.2",
    "is-number": "^7.0.0",
    "isotope-layout": "^3.0.6",
    "lazysizes": "^5.3.2",
    "photoswipe": "^4.1.3",
    "raf-schd": "^4.0.3",
    "react-ace": "^9.5.0",
    "react-masonry-component": "^6.3.0",
    "react-select": "^5.2.1",
    "react-sortable-hoc": "^2.0.0",
    "shorthash": "0.0.2",
    "simplebar": "^5.3.6",
    "swiper": "^8.4.7",
    "throttle-debounce": "^3.0.1"
  }
}
